{
    "name": "teamzac/zactax-php",
    "description": "The official PHP client for the ZacTax API",
    "type": "library",
    "version": "1.0",
    "require": {
        "php": ">=5.5.9",
        "nesbot/carbon": "~1.20",
        "illuminate/support": "^5.4",
        "guzzlehttp/guzzle": "6.*"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.7",
        "phpunit/phpunit": "^5.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Janicek",
            "email": "chad@zactax.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TeamZac\\ZacTax\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TeamZac\\ZacTax\\Test\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors"
    }
}
